<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.SecondWindow">
   <children>
      <Label layoutX="240.0" layoutY="56.0" text="New Game" textAlignment="CENTER">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button layoutX="510.0" layoutY="333.0" mnemonicParsing="false" onAction="#btnReturnClicked" text="return" />
      <MenuButton fx:id="menuButton" layoutX="477.0" layoutY="56.0" mnemonicParsing="false" onAction="#btnMenuClicked" prefHeight="38.0" prefWidth="96.0" text="Menu">
        <items>
          <MenuItem fx:id="menuItem1" mnemonicParsing="false" onAction="#btnMenuitem1Clicked" text=" heroes" />
          <MenuItem fx:id="menuItem2" mnemonicParsing="false" onAction="#btnMenitem2Onclick" text="inventory" />
        </items>
      </MenuButton>
      <ImageView fitHeight="400.0" fitWidth="602.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
